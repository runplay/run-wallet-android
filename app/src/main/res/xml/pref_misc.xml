        android:key="preference_nudge_attempts"
        android:summary="@string/settings_auto_nudge_summary"
        android:title="@string/settings_auto_nudge" />
    <ListPreference
        android:defaultValue="50"
        android:entries="@array/firstLoadAttempts"
        android:entryValues="@array/firstLoadAttempts"
        android:key="preference_fl_attempts"
        android:summary="@string/settings_first_load_summary"
        android:title="@string/settings_first_load" />
    <ListPreference
